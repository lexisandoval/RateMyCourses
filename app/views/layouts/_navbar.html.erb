<% if !logged_in? %>
  <div class="nav">
    <ul>
      <li style ="padding-right: 3%"><%= link_to "Log In", login_path %></li>
      <li><%= link_to "Sign Up", signup_path %></li>
      <li style="float:left"><a class="main brand" href="/">RateMyCourses</a></li>
    </ul>
  </div>
<% else %>
  <div class="nav">
    <ul>
      <li style ="padding-right: 3%"><%= link_to "Log Out", logout_path, method:"delete"%></li>
      <li><%= link_to "Profile", user_path(current_user) %></li>
      <li><%= link_to "Rate", new_user_rating_path(current_user) %></li>
      <li><%= link_to "Home", ratings_path %></li>
      <li style="float:left"><a class="main brand" href="/ratings">RateMyCourses</a></li>
    </ul>
  </div>
<% end %>