<h1 class="brand head">Log In</h1><br>

<%= form_for(:user, :html => { class: 'form' }) do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br><p class="smol"></p>
  
  <%= f.label :password %><br>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit "Log In" %>
<% end %>
<br>
<h5>or</h5><br>

<h3 class="form"><%= button_to "Log In With Google", '/auth/google_oauth2' %></h3><br>
<p><a class="black no_underline main brand" href="/">Go Back</a></p>